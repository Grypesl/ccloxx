fun line_conf(a, b) {
    class Cat
    {
        func eat() {
            return 666;
        }
    }

    //////////////////////////////
    // Use class member do stuff
    //////////////////////////////
    var cat = Cat();
    var fish = cat.eat();

    fun line(x) {
        return a*x + b + fish;
    }
    return line;
}

var line1 = line_conf(1, -1);
print line1(1); // "666"