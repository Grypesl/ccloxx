// ----------- closure  -----------

fun line_conf(a, b) {
    fun line(x) {
        return a*x + b;
    }
    return line;
}

var line1 = line_conf(1, 1);
var line2 = line_conf(4, 5);
print line1(5); // "6".
print line2(4); // "21".